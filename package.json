{
  "name": "trajectory-reconstruction",
  "version": "1.0.0",
  "type": "module",
  "description": "Trajectory reconstruction in the browser with OpenCV and Ceres",
  "repository": {
    "type": "git",
    "url": "git://github.com/eigendude/trajectory-reconstruction.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "science"
  ],
  "scripts": {
    "audit-ci": "audit-ci --config audit-ci.json",
    "build": "snowpack build",
    "clean": "rimraf build node_modules web_modules",
    "depends": "bash tools/npm-depends.sh all",
    "depends:build": "bash tools/npm-depends.sh build",
    "depends:checkout": "bash tools/npm-depends.sh checkout",
    "depends:install": "bash tools/npm-depends.sh install",
    "deploy": "gh-pages --branch main --repo https://github.com/eigendude/eigendude.github.io.git --dist build --dotfiles",
    "format": "prettier --write . && eslint --fix .",
    "lint": "prettier --check . && eslint .",
    "start": "snowpack dev",
    "test": "mocha --loader=ts-node/esm/transpile-only"
  },
  "dependencies": {
    "hls.js": "^1.5.6",
    "jsonld": "^8.3.2",
    "three": "^0.161.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "audit-ci": "^6.6.1",
    "chai": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "gh-pages": "^6.1.1",
    "glob-parent": "^6.0.2",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "prettier-plugin-pkg": "^0.18.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "snowpack": "^3.8.8",
    "snowpack-plugin-copy": "^1.0.1",
    "threads": "^1.7.0",
    "threads-plugin": "^1.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "glob-parent": "6.0.2",
    "rollup": "4.12.0",
    "tough-cookie": "4.1.3",
    "vm2": "3.9.19"
  },
  "bundleDependencies": false,
  "target": "web"
}
